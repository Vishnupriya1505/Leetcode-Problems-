import java.util.HashSet;
class Solution {
    public int distributeCandies(int[] candyType) {
        HashSet<Integer> set = new HashSet<>();
        for(int i : candyType) {
            set.add(i);
        }
        int maxEat = candyType.length / 2;
        return Math.min(set.size(), maxEat);
    }
}
