class Solution {
    public boolean isFascinating(int n) {
        String concat = "" + n + (n * 2) + (n * 3);
        if (concat.length() != 9) {
            return false;
        }
        boolean[] seen = new boolean[10];
        for (char c : concat.toCharArray()) {
            int digit = c - '0';
            if (digit == 0 || seen[digit]) {
                return false;
            }
            seen[digit] = true;
        }
        return true;
    }
}
